	langid = {english},
	keywords = {Condensed Matter - Soft Condensed Matter, Physics - Optics},
	file = {Wang et al. - 2014 - Using the Discrete Dipole Approximation and Hologr.pdf:C\:\\Users\\maxim\\Zotero\\storage\\7P8ETZPX\\Wang et al. - 2014 - Using the Discrete Dipole Approximation and Hologr.pdf:application/pdf;Wang et al. - 2014 - Using the Discrete Dipole Approximation and Hologr.pdf:C\:\\Users\\maxim\\Zotero\\storage\\I3B879VS\\Wang et al. - 2014 - Using the Discrete Dipole Approximation and Hologr.pdf:application/pdf},
