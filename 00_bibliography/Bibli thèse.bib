	journaltitle = {Journal of Quantitative Spectroscopy and Radiative Transfer},
	shortjournal = {Journal of Quantitative Spectroscopy and Radiative Transfer},
	author = {Wang, Anna and Dimiduk, Thomas G. and Fung, Jerome and Razavi, Sepideh and Kretzschmar, Ilona and Chaudhary, Kundan and Manoharan, Vinothan N.},
